         *  SqlConnection myADONETConnection = (SqlConnection)rawConnection;
         *  //Use the connection in some code here, then release the connection
         *  Dts.Connections["Sales DB"].ReleaseConnection(rawConnection);
         *
         * Example of using a File connection manager
         *  object rawConnection = Dts.Connections["Prices.zip"].AcquireConnection(Dts.Transaction);
         *  string filePath = (string)rawConnection;
         *  //Use the connection in some code here, then release the connection
         *  Dts.Connections["Prices.zip"].ReleaseConnection(rawConnection);
         * */
        #endregion


		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{
            try
            {
                // TODO: Add your code here
                //declare required variables
                String folder_path = "", file_name = "", file_extension = "", query, job_name;
                job_name = Dts.Variables["JOB_NAME"].Value.ToString();

                //Configure a SQL Connection in Script Task
                SqlConnection conn = new SqlConnection();
                conn = (SqlConnection)(Dts.Connections["CCDW_STG ADO"].AcquireConnection(Dts.Transaction) as SqlConnection);

                //DECLARE query
                query = "SELECT * FROM [CCDW_STG].[dbo].[RAW_FILE_DETAILS] where JOB_NAME= '" + job_name + "'";

                //execute the query and save the data in the required variables
                System.Data.SqlClient.SqlCommand sqlComm;
                sqlComm = new System.Data.SqlClient.SqlCommand(query, conn);
                using (SqlDataReader oReader = sqlComm.ExecuteReader())
                {
                    while (oReader.Read())
                    {
                        folder_path = oReader["FOLDER_LOCATION"].ToString();
                        file_name = oReader["FILE_NAME"].ToString();
                        file_extension = oReader["FILE_EXTENSION"].ToString();
                    }
                    conn.Close(); //close ADO connection
                }

                //set FILE_PATTERN and SOURCE_PATH
                Dts.Variables["SOURCE_PATH"].Value = folder_path;
                Dts.Variables["FILE_PATTERN"].Value = "*" + file_name + "*" + file_extension;

                //MessageBox.Show(Dts.Variables["FILE_PATTERN"].Value.ToString());
                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception e)
                {
                throw e;
            }
		}

