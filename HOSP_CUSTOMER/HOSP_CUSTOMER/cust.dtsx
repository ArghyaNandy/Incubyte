		public void Main()
		{
            try
            {

                // TODO: Add your code here
                String source = Dts.Variables["SOURCE_PATH"].Value.ToString();
                String archieve = source + @"\Archieve\";

                //find all files in source
                String[] fileNames = Directory.GetFiles(source);

                //take all the files in for loop and perform action
                foreach (string inputFile in fileNames)
                {
                    String archieve_file_name = Path.Combine(archieve, Path.GetFileName(inputFile));
                    File.Move(inputFile, archieve_file_name);   //move file in destination
